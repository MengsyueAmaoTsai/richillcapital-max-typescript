{
  "name": "richillcapital-max",
  "version": "1.0.3",
  "description": "MAX(Taiwan) crypto exchange API SDK for TypeSciprt.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "rest:market": "ts-node ./examples/market-data-client.ts",
    "rest:trading": "ts-node ./examples/trading-client.ts",
    "rest": "npm run rest:market && npm run rest:trading",
    "ws:market": "ts-node ./examples/market-data-client-ws.ts",
    "ws:trading": "ts-node ./examples/trading-client-ws.ts",
    "feed": "ts-node ./examples/datafeed.ts",
    "broker": "ts-node ./examples/brokerage.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "npm run clean && tsc --watch",
    "release": "npm publish"
  },
  "author": "Mengsyue Amao Tsai <mengsyue.tsai@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "qs": "^6.11.0",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "@types/webpack": "^5.28.0",
    "@types/ws": "^8.5.4",
    "dotenv": "^16.0.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
