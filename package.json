{
  "name": "richillcapital-max",
  "version": "1.0.9",
  "description": "MAX(Taiwan) crypto exchange API SDK for TypeSciprt.",
  "keywords": [
    "max",
    "max api",
    "maicoin api",
    "maicoin",
    "spot",
    "api",
    "websocket",
    "rest",
    "rest api",
    "crypto",
    "cryptocurrency",
    "usdt",
    "btc",
    "nodejs",
    "typescript",
    "javascript",
    "bitcoin",
    "eth",
    "richillcapital"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MengsyueAmaoTsai/richillcapital-max-typescript"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/*"
  ],
  "scripts": {
    "rest:m": "ts-node ./examples/market-data-rest.ts",
    "rest:t": "ts-node ./examples/trading-rest.ts",
    "ws:m": "ts-node ./examples/market-data-ws.ts",
    "ws:t": "ts-node ./examples/trading-ws.ts",
    
    "feed": "ts-node ./examples/datafeed.ts",
    "broker": "ts-node ./examples/brokerage.ts",

    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "npm run clean && tsc --watch",
    "pack": "webpack --config webpack/webpack.config.js",
    "publish:build": "npm run build:clean && npm publish"
  },
  "author": "Mengsyue Amao Tsai <mengsyue.tsai@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "qs": "^6.11.0",
    "winston": "^3.8.2",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "@types/webpack": "^5.28.0",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.5.4",
    "dotenv": "^16.0.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
